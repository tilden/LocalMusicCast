{
  "name": "Local Music Cast",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "31",
  "app": {
    "background": {
      "scripts": ["tcp-server.js", 
                  "commands/BrowserCommands.js", 
                  "main_background.js"]
    }
  },
  "permissions": [
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "storage", 
    "webview", 
    "system.network"
  ],
  "file_handlers": {
    "mp3": {
        "types": [
            "audio/mpeg3",
            "audio/x-mpeg-3",
            "video/mpeg",
            "video/x-mpeg",
            "audio/mpeg"
        ],
        "extensions": [
            "mp3"
        ],
        "title": "LocalMusicCast"
    }
  }, 
  "sockets": {
    "tcpServer": {
      "listen": ""
    }
  }
}
